
public class MyJavaFile {

	/**
	 * This class asks the user for their name and outputs a nice message.
	 * @param args the name(s) that have been input.
	 */
	public static void main(String[] args) {
	
     System.out.print("What is your name?");
     for (int i=0;i < args.length;i++){
    	 System.out.print("Hi" +args[i]+ " I hope you are keeping well");
     }
    
   }
}